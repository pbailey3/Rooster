        <th>
            @Html.DisplayNameFor(model => model.AddressState)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddressPostcode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeIndustry)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeDetail)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressLine1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressLine2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressSuburb)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressState)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddressPostcode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeIndustry)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeDetail)
        </td>
        <td>
            @Html.ActionLink("Locations", "InternalLocationIndex", new { businessid=item.Id }) |
            @Html.ActionLink("Roles", "RoleIndex", new { businessid=item.Id }) |
            @Html.ActionLink("Preferences", "PreferencesDetails", new { businessid=item.Id }) |
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
